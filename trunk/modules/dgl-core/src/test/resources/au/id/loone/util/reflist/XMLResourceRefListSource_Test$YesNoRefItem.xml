<rl:refList
    description="Yes/No reference list for testing."
    name="TestYesNoRefItem"
    version="1"
    xmlns="http://loone.id.au/xml/refList_v1.xsd"
    xmlns:rl="http://loone.id.au/xml/refList_v1.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
  >

  <rl:implementation>
    <rl:java class="au.id.loone.util.reflist.XMLResourceRefListSource_Test$YesNoRefItem"/>
  </rl:implementation>

  <rl:propertyDefs>
    <rl:propertyDef name="flag" type="boolean"/>
    <rl:propertyDef indexed="true" name="nums" type="int"/>
  </rl:propertyDefs>

  <rl:item code="NO" description="No" obsolete="false">
    <rl:alias value="N"/>
    <rl:property name="flag" value="false"/>
  </rl:item>

  <rl:item code="YES" description="Yes" obsolete="false">
    <rl:alias value="Y"/>
    <rl:alias value="y"/>
    <rl:property name="flag" value="true"/>
  </rl:item>

  <rl:item code="UNKNOWN" description="Unknown" obsolete="true"/>

</rl:refList>
